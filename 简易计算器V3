#include <stdio.h>
#include <string.h>
int main() 
{
	char str[]="2+2*3+2/2-1";
	char str1[100]="\0";
	int count=0;
	for(int i=0;i<strlen(str);i++)
	{
		if(str[i]=='*')
		{
			int right=str[i+1]-'0';
			int left=str[i-1]-'0';
			char res1=right*left+'0';
			str1[--count]=res1;
			i++;count++;
		}
		else if(str[i]=='/')
		{
			int right=str[i+1]-'0';
			int left=str[i-1]-'0';
			char res2=left/right+'0';
			str1[--count]=res2;
			i++;count++;
		}
		else
		{
			str1[count]=str[i];
			count++;
			
		}
	}
	int res=str1[0]-'0';
	for(int j=0;j<count;j++)
	{
		if(str1[j]=='+')
		{
			int right=str1[j+1]-'0';
			res=res+right;
		}
		else if(str1[j]=='-')
		{
			int right=str1[j+1]-'0';
			res=res-right;
			
		}
	}
	printf("%d",res);
	return 0;
}
